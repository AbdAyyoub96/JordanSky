@model IEnumerable<JordanSky.Entity.Register>

@{
    ViewBag.Title = "Confirmed_Request";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="card">
    <div class="card-header header-elements-inline">
        <h2 class="card-title">Confirmed Request</h2>
        <div class="header-elements">
            <div class="list-icons">
                <a class="list-icons-item" data-action="collapse"></a>
            </div>

        </div>
    </div>
    <table class="table datatable-show-all">
        <thead>
            <tr>

                <th>Name</th>
                <th>Phone</th>
                <th>City</th>
                <th>Location</th>
                <th>Picture</th>
                <th>license</th>
                <th class="text-center">Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Phone</td>
                    <td>@item.city.Name</td>
                    <td>@item.Location</td>
                    <td><a href="~/Image_Facilty/@item.Picture"><i class="icon-eye4"></i></a></td>
                    <td><a href="~/Image_Facilty/@item.license"><i class="icon-eye4"></i></a></td>
                    <td class="text-center">
                        <div class="list-icons">
                            <a href="@Url.Action("Delete","Farm_Requests",new {id= item.Id })" class="btn btn-outline-dark" id=""><i class="text-blue-800    icon-cross2 position-left"></i></a>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
